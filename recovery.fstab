# mount     fstype  device                  [device2]   [fstype2]   [fsoptions]       [fsoptions2]

/boot       bml     boot
/recovery   bml     recovery

/cache      ext4    /dev/block/mmcblk0p1
/data       ext4    /dev/block/mmcblk0p2	length=-16384
/system     ext4    /dev/block/stl9
/sdcard     vfat    /dev/block/mmcblk1p1

